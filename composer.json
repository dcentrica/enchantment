{
    "name": "goldfinch/enchantment",
    "description": "Customly styled admin theme for Silverstripe, based on Bootstrap 5",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "silverstripe-admin-theme",
        "admin-theme",
        "silverstripe-bootstrap5",
        "admin-bootstrap5",
        "ss-admin"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Art Karasev",
        "email": "art.karasev@proton.me"
    }],
    "require": {
        "php": ">=8.0",
        "silverstripe/framework": "^5.0",
        "silverstripe/admin": "^2.0",
        "goldfinch/extra-assets": "^1.0"
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "Goldfinch\\Enchantment\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
